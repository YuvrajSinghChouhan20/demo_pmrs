<div class="container " id="listing-container">
  <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <th>Images</th>
          <th>Status</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <% properties.each do |property|%>
          <tr>
            <td><%= property.property_name%></td>
            <td><%= property.status %></td>
            <td>
              <%= link_to "See Bookings", "#{property_path(property.id)}/bookings" %>
              <%= link_to "Edit", edit_property_path(property) %>
              <%= button_to "Destroy", property_path(property), method: :delete %>
            </td>
          </tr>
        <%end%>
      </tbody>
    </table>
  </div>
</div>

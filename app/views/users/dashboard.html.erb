<% content_for :stylesheet do %>
  <%= stylesheet_link_tag "dashboard", "data-turbo-track": "reload"%>
<%end%>
<div class="container-fluid ">
  <div class="row dashboard-container p-3 m-0 rounded-4">
    <div class="col-md-2 right-pane border-opacity-10 d-flex p-0">
      <ul class="dashboard-side-menu p-0 mt-5">
        <li>
          <button class="btn dashboard-btn " data-bs-dashboard-btn="dashboard" id="dashboard" >Dashboard</button>
        </li>
        <li>
          <button class="btn dashboard-btn " data-bs-dashboard-btn="properties" id="" >My Listed Properties</button>
        </li>
        <li>
          <button class="btn dashboard-btn" data-bs-dashboard-btn="rented-property">Rented Property</button>
        </li>
        <li>
          <button class="btn dashboard-btn" data-bs-dashboard-btn="bookings" id="" >My Bookings</button>
        </li>
        <li>
          <button class="btn dashboard-btn" data-bs-dashboard-btn="notifications" id="" >Notifications</button>
        </li>
        <li>
          <button class="btn dashboard-btn" data-bs-dashboard-btn="notifications" id="" >Pending Agreements</button>
        </li>
      </ul>
    </div>
    <div class="col-md-10 left-pane bg-light h-100 rounded-5 px-2 py-4">
      <div class="data" id="dashboard-user-data">
        <%= render 'dashboard'%>
      </div>
    </div>
  </div>
</div>